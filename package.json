{
  "name": "nfm",
  "version": "1.0.0",
  "description": "node file manager",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=*,-babel nodemon -V -i logs/ -i assets.json -i public/ app.js",
    "build": "NODE_ENV=production webpack --optimize-dedupe --optimize-minimize --optimize-occurrence-order --progress --colors --config webpack.config.js",
    "predeploy": "npm run build",
    "deploy": "npm pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keenwon/nfm.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "co-body": "^4.2.0",
    "koa": "^1.2.0",
    "koa-favicon": "^1.2.1",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "moment": "^2.13.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "koa-webpack-dev-middleware": "^1.2.1",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-md5-hash": "0.0.5"
  },
  "files": [
    "assets/favicon.ico",
    "assets/app.*.js",
    "controllers",
    "lib",
    "views",
    "*.js",
    "*.json",
    "!assets/app.js",
    "!webpack.config.js",
    "!.eslintrc.json"
  ]
}
