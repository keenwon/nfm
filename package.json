{
  "name": "nfm",
  "version": "1.0.0",
  "description": "node file manager",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "build": "NODE_ENV=production webpack --optimize-dedupe --optimize-occurrence-order --progress --colors --display-error-details --config assets/webpack.config.js",
    "test": "istanbul cover -x **/logger.js _mocha -- \"test/**/*.spec.js\" -R spec && istanbul report text && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "predeploy": "npm test && npm run build",
    "deploy": "npm pack",
    "postdeploy": "rm -vrf assets/script/bundle.*.js assets/script/vendor.*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keenwon/nfm.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "co-body": "^4.2.0",
    "execa": "^0.4.0",
    "filesize": "^3.3.0",
    "formidable": "^1.0.17",
    "fs-extra": "^0.30.0",
    "koa": "^1.2.2",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mime-types": "^2.1.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "coveralls": "^2.11.12",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "form-data": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "koa-webpack-dev-middleware": "^1.2.2",
    "lodash.merge": "^4.6.0",
    "mocha": "^3.0.2",
    "mocha-eslint": "^3.0.1",
    "nodemon": "^1.10.2",
    "normalizr": "^2.2.1",
    "proxyquire": "^1.7.10",
    "react": "^15.3.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "webpack": "^1.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "files": [
    "assets/script/bundle.*.js",
    "assets/script/vendor.*.js",
    "assets/favicon.ico",
    "assets/.assets.json",
    "controllers",
    "lib",
    "utils",
    "views",
    "process.json"
  ]
}
