{
  "name": "nfm",
  "version": "1.0.0",
  "description": "node file manager",
  "main": "lib/app.js",
  "private": true,
  "scripts": {
    "start": "nodemon lib/app.js",
    "build": "NODE_ENV=production webpack --optimize-dedupe --optimize-occurrence-order --progress --colors --display-error-details --config assets/webpack.config.js",
    "test": "istanbul cover -i **/controllers/**/*.js _mocha test/**/*.spec.js && istanbul report text",
    "predeploy": "npm test && npm run build",
    "deploy": "npm pack",
    "postdeploy": "rm -vrf assets/script/bundle.*.js assets/script/vendor.*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keenwon/nfm.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.1",
    "co-body": "^4.2.0",
    "filesize": "^3.3.0",
    "fs-extra": "^0.30.0",
    "koa": "^1.2.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "moment": "^2.13.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "classnames": "^2.2.5",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.1.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.3",
    "koa-webpack-dev-middleware": "^1.2.1",
    "lodash.merge": "^4.4.0",
    "mocha": "^2.5.3",
    "mocha-eslint": "^2.1.0",
    "nodemon": "^1.9.2",
    "normalizr": "^2.1.0",
    "proxyquire": "^1.7.10",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "webpack": "^1.13.1",
    "webpack-md5-hash": "0.0.5"
  },
  "files": [
    "assets/script/bundle.*.js",
    "assets/script/vendor.*.js",
    "assets/favicon.ico",
    "assets/.assets.json",
    "controllers",
    "lib",
    "utils",
    "views",
    "process.json"
  ]
}
